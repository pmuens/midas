root = true  ; top-most EditorConfig file

[*]
indent_style = space
end_of_line = lf
indent_size = 2
charset = utf-8
quote_type = single
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
max_line_length = 0
trim_trailing_whitespace = false

[*.sol]
indent_size = 4
quote_type = double
